{
  "name": "ai-playlist-generator",
  "version": "1.10.1",
  "private": true,
  "packageManager": "yarn@3.8.0",
  "scripts": {
    "dev": "next dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "test-storybook --ci",
    "test-storybook:update": "test-storybook -u",
    "test-storybook:serve": "concurrently -k -s first -n \"SB,TEST\" \"http-server storybook-static -p 6006 -s\" \"wait-on tcp:127.0.0.1:6006 && yarn test-storybook\"",
    "test-storybook:serve-update": "concurrently -k -s first -n \"SB,TEST\" \"http-server storybook-static -p 6006 -s\" \"wait-on tcp:127.0.0.1:6006 && yarn test-storybook -u\"",
    "build:worker": "node scripts/build-metadata-worker.js",
    "build": "yarn build:worker && next build",
    "start": "next start",
    "lint": "eslint .",
    "clean": "rm -rf .next",
    "dev:clean": "rm -rf .next && next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "version": "node scripts/version.js",
    "release-notes": "node scripts/release-notes.js",
    "update-changelog": "node scripts/update-changelog.js"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-tabs": "^1.1.13",
    "@visx/axis": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/gradient": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "browser-id3-writer": "^6.3.1",
    "clsx": "^2.1.1",
    "d3-shape": "^3.2.0",
    "dexie": "^4.2.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.561.0",
    "music-metadata": "^11.10.3",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.25.0",
    "@babel/preset-typescript": "^7.25.0",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/nextjs-vite": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@storybook/test": "^8.6.0",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.2.0",
    "concurrently": "^9.1.0",
    "esbuild": "^0.27.2",
    "eslint": "^9",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-storybook": "10.2.8",
    "http-server": "^14.1.1",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.5.0",
    "playwright": "^1.49.0",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.2.0",
    "sharp": "^0.34.5",
    "storybook": "^10.2.8",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3",
    "vite": "^5.4.0",
    "wait-on": "^8.0.2"
  }
}
